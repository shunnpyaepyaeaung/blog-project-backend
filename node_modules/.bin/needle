#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

export NODE_PATH="/Users/shunnpyaepyaeaung/Desktop/Projects/fullstack-post/backend/node_modules/needle/bin/needle/node_modules:/Users/shunnpyaepyaeaung/Desktop/Projects/fullstack-post/backend/node_modules/needle/bin/node_modules:/Users/shunnpyaepyaeaung/Desktop/Projects/fullstack-post/backend/node_modules/needle/node_modules:/Users/shunnpyaepyaeaung/Desktop/Projects/fullstack-post/backend/node_modules:/Users/shunnpyaepyaeaung/Desktop/Projects/fullstack-post/node_modules:/Users/shunnpyaepyaeaung/Desktop/Projects/node_modules:/Users/shunnpyaepyaeaung/Desktop/node_modules:/Users/shunnpyaepyaeaung/node_modules:/Users/node_modules:/node_modules"
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../needle/bin/needle" "$@"
else 
  exec node  "$basedir/../needle/bin/needle" "$@"
fi
